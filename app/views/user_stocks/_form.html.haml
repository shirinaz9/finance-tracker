= form_for @user_stock do |f|
  - if @user_stock.errors.any?
    #error_explanation
      %h2= "#{pluralize(@user_stock.errors.count, "error")} prohibited this user_stock from being saved:"
      %ul
        - @user_stock.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :user_id
    = f.number_field :user_id
  .field
    = f.label :stock_id
    = f.number_field :stock_id
  .actions
    = f.submit 'Save'
